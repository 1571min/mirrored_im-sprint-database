{
  "name": "databases",
  "version": "1.1.0",
  "description": "Learn about databases",
  "private": true,
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "start": "node ./server/app.js",
    "precommit": "lint-staged",
    "start:es6": "babel-node ./server/app.js",
    "test": "mocha server/spec/server-spec.js --timeout 20000 --exit",
    "test:es6": "mocha --compilers js:babel-register server/spec/server-spec.js; RES=$?; kill %1; exit $RES",
    "lint": "eslint ./"
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "eslint",
        "git add"
      ]
    }
  },
  "dependencies": {
    "body-parser": "^1.9.2",
    "express": "^4.10.1",
    "morgan": "^1.4.1",
    "mysql": "^2.4.2",
    "request": "^2.69.0",
    "sequelize": "^3.21.0"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "eslint": "^5.7.0",
    "eslint-config-codestates": "^1.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "mocha": "^2.4.5",
    "husky": "^1.0.1",
    "lint-staged": "^7.3.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
